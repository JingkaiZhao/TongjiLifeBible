<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="edu.tongji.sp.sitp.pojo">
	<class name="Leisure" lazy="false">
		<id name="id" type="java.lang.Integer">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="name"></column>
		</property>
		<property name="pcc" type="java.lang.String">
			<column name="pcc"></column>
		</property>
		<property name="tel" type="java.lang.String">
			<column name="tel"></column>
		</property>
		<property name="address" type="java.lang.String">
			<column name="address"></column>
		</property>
		<property name="sort" type="java.lang.String">
			<column name="sort"></column>
		</property>
		<property name="profile" type="java.lang.String">
			<column name="profile" sql-type="TEXT"></column>
		</property>
		<property name="lat" type="java.lang.String">
			<column name="lat"></column>
		</property>
		<property name="lng" type="java.lang.String">
			<column name="lng"></column>
		</property>
	</class>
</hibernate-mapping>